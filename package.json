{
  "name": "nexteranga-stack",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter=!@nexteranga/docs && changeset publish",
    "prepare": "husky"
  },
  "devDependencies": {
    "@nexteranga/config": "workspace:*",
    "@changesets/cli": "^2.27.1",
    "husky": "^9.0.0",
    "prettier": "^3.7.4",
    "turbo": "^2.6.3",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  }
}
